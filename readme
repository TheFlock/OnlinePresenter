






com/display/GestureOverLayView

GestureOverLayView   ( Static Class )

//usage

GestureOverLayView.init ( wrapperElement );
GestureOverLayView.openViewByType( type );     type are : "swipeUp"  "swipeDown" , "swipeLeft" , "swipeRight"  "mouseShake"  "mobileShake" "tap" "tapVig"
GestureOverLayView.closeView();
GestureOverLayView.destroy();



com/display/HotspotView

HotspotView   ( Static Class )

//usage

HotspotView.init ( wrapperElement );
HotspotView.setPosition( array )        [ x , y]
HotspotView.openView();
HotspotView.closeView();
HotspotView.destroy();



